{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development nodemon --ext ts --exec ts-node src/server.ts",
    "db:migrate:dev": "cp .env.development.local .env && prisma migrate dev --schema=prisma/schema.dev.prisma",
    "db:migrate:prod": "cp .env.production.local .env && prisma migrate deploy",
    "db:seed:dev": "cp .env.development.local .env && prisma db seed --schema=prisma/schema.dev.prisma",
    "db:seed:prod": "cp .env.production.local .env && prisma db seed",
    "db:studio": "cp .env.development.local .env && prisma studio --schema=prisma/schema.dev.prisma"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "winston": "^3.17.0"
  }
}
